{
  "project": {
    "title": "invert-bit project",

    "flows": {
      "invert-bit": {
        "type": "root",
        "name": "invert bit",
        "ports": {
          "in": {
            "type": "in",
            "dataType": "boolean"
          }
        },

        "nodes": {
          "inverter": {
            "type": "code",
            "name": "inverter",
            "block": {
              "code": "export default function({input}) {\nreturn { out: !input };\n}"
            },
            "ports": {
              "input": {
                "type": "in",
                "dataType": "boolean"
              },
              "out": {
                "type": "out",
                "dataType": "boolean",
                "links": [
                  {
                    "nodeID": "node-2",
                    "portID": "data"
                  }
                ]
              }
            }
          },

          "node-2": {
            "type": "block",
            "name": "out",
            "block": {
              "name": "test.blocks.printer"
            },
            "ports": {
              "data": {
                "type": "in",
                "dataType": "boolean"
              }
            }
          }
        }
      }
    },

    "imports": {
      "library.1": "./library1.json",
      "library.2-sel": {
        "url": "./library2.js",
        "import": "AESBlockCipher, DESBlockCipher"
      },
      "library.12": [
        "./library1.json",
        "./library2.js"
      ],
      "library.12-sel": {
        "url": [
          "./library1.json",
          "./library2.js"
        ],
        "import": "AESBlockCipher, DESBlockCipher"
      }
    },
    
    "blocks": {
      "invertor-block" : {
        "type": "code",
        "name": "Inverting Block",
        "code": "export default function({input}) {\nreturn { out: !input };\n}",

        "properties": {
          "input": {
            "type": "in",
            "dataType": "boolean"
          },
          "out": {
            "type": "out",
            "dataType": "boolean"
          }
        }
      }
    }
  }
}